{
	"name": "discord-delivery",
	"version": "1.0.0",
	"description": "Helpful tool for read-only channels in Discord servers.",
	"keywords": [
		"Discord Delivery",
		"Discord"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Jiralite/Discord-Delivery.git"
	},
	"license": "MIT",
	"author": "Jiralite",
	"type": "module",
	"exports": "./distribution/index.js",
	"types": "./distribution/index.d.ts",
	"files": [
		"distribution"
	],
	"scripts": {
		"build": "tsdown",
		"format": "biome check --write",
		"lint": "biome check",
		"prepack": "pnpm run build && pnpm run lint",
		"test": "tsc --noEmit",
		"update": "pnpm update --interactive --latest"
	},
	"dependencies": {
		"@discordjs/core": "^2.2.0",
		"@discordjs/rest": "^2.5.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.3",
		"tsdown": "^0.13.3",
		"typescript": "^5.9.2"
	},
	"devEngines": {
		"runtime": {
			"name": "node",
			"version": ">=22",
			"onFail": "error"
		},
		"packageManager": {
			"name": "pnpm",
			"version": ">=10.14.0",
			"onFail": "error"
		}
	}
}
