{
	"name": "discord-delivery",
	"version": "1.0.2",
	"description": "Helpful tool for read-only channels in Discord servers.",
	"keywords": [
		"Discord Delivery",
		"Discord"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Jiralite/Discord-Delivery.git"
	},
	"license": "MIT",
	"author": "Jiralite",
	"type": "module",
	"exports": "./distribution/index.js",
	"types": "./distribution/index.d.ts",
	"files": [
		"distribution"
	],
	"scripts": {
		"build": "tsup",
		"format": "biome check --write",
		"lint": "biome check",
		"prepack": "pnpm run build && pnpm run lint",
		"test": "tsc --noEmit",
		"update": "pnpm update --interactive --latest"
	},
	"dependencies": {
		"@discordjs/core": "dev",
		"@discordjs/rest": "dev"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.7",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3"
	},
	"devEngines": {
		"runtime": {
			"name": "node",
			"version": ">=24",
			"onFail": "error"
		},
		"packageManager": {
			"name": "pnpm",
			"version": ">=10.23.0",
			"onFail": "error"
		}
	}
}
